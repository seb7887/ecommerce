    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@welldone-software/why-did-you-render": "^3.3.7",
    "autoprefixer": "^9.6.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "fast-sass-loader": "^1.5.0",
    "file-loader": "^4.2.0",
    "firebase-tools": "^7.6.1",
